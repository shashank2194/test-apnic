    "@types/node": "16.11.26",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.13",
    "axios": "0.26.1",
    "bootstrap": "5.1.3",
    "prettier": "2.5.1",
    "react": "17.0.2",
    "react-app-rewired": "2.2.1",
    "react-bootstrap": "2.2.0",
    "react-dom": "17.0.2",
    "react-scripts": "5.0.0",
    "sort-package-json": "1.54.0",
